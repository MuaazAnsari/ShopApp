<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length > 0) { %>
                    <ul>
                        <% for (order of orders) {%>
                            <h1> #<%= order.id %></h1>
                            <% for (product of order.products) {%>
                                <li>
                                    <%= product.title %> (<%= product.orderItem.quantity %>)
                                </li>
                            <% } %>
                        <% } %>
                    </ul>
            <% } else { %>
            <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>