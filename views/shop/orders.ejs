<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length > 0) { %>
                    <ul>
                        <% for(order of orders) { %>
                            <h1> #<%= order._id %></h1>
                            <% for (p of order.products) {%>
                                <li>
                                    <%= p.product.title %> (<%= p.quantity %>)
                                </li>
                            <% } %>
                        <% } %>
                    </ul>
            <% } else { %>
            <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>